        </sorting-attributes>
    </sorting-rule>

    <sorting-rule rule-id="featured">
        <description>Manual Placement then What's Hot</description>
        <sorting-attributes>
            <category-position/>
            <attribute>
                <attribute-path>product.activeData.revenueWeek</attribute-path>
                <direction>descending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
            <attribute>
                <attribute-path>product.activeData.ordersWeek</attribute-path>
                <direction>descending</direction>
                <include-text-relevance>false</include-text-relevance>
            </attribute>
        </sorting-attributes>
    </sorting-rule>

